package com.manbodh;

public class Main {

	public static void main(String[] args) {

		Car a = new MustangGT97(4, "MustangGT", "Ford", true, false, false);
		Car b = new DodgeRam1500(4, "Ram 1500", "Dodge", false, true, false);
		Car c = new Tesla314(4, "Tesla 314", "Tesla", false, false, true);
		
		System.out.println(a.getModel());
	System.out.println(b.getModel());
	System.out.println(c.getModel());
	}

}
