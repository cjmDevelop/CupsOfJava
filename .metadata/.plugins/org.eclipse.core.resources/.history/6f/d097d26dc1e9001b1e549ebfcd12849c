package myEncapsulation;

public class MyEncap {
	
	private String name;
	private int age;
	
	public MyEncap( int age) {
		super();
	
		this.age = age;
	}

	public String getName() {
		return name;
	}

	public void setName(String name) {
		this.name = name;
	}

	public int getAge() {
		return age;
	}

	public void setAge(int age) {
		this.age = age;
	}
	
	
	
	

}
