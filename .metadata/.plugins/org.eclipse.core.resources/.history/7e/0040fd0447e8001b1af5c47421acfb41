package com.manbodh;

public class Axis extends Bank {





private float money;
private float interest;
	

	
	
	public Axis(float money, float interest, float money2, float interest2) {
	super(money, interest);
	money = money2;
	interest = interest2;
}




	public void getRateOfInterest() {
		System.out.println(money * interest);
	}


}
